{
  "version": 2,
  "builds": [
    { "src": "server/server.js", "use": "@vercel/node" },
    { "src": "e-commerce/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server/server.js" },
    { "src": "/assets/(.*)", "dest": "/e-commerce/assets/$1" },
    { "src": "/(.*)", "dest": "/e-commerce/index.html" }
  ]
}